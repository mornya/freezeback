{
  "name": "freezeback",
  "version": "1.0.0",
  "description": "The project for handling the 'Back button pressed' event.",
  "keywords": [
    "freezeback",
    "vessel",
    "back",
    "button",
    "key",
    "press",
    "event",
    "javascript",
    "ecmascript",
    "typescript",
    "front-end"
  ],
  "homepage": "https://github.com/mornya/freezeback",
  "bugs": "https://github.com/mornya/freezeback/issues",
  "license": "MIT",
  "author": "mornya <mornya78@gmail.com>",
  "private": false,
  "repository": "https://github.com/mornya/freezeback.git",
  "main": "./dist/index.js",
  "types": "./dist",
  "scripts": {
    "setup": "husky install && lintest install || exit 0",
    "prepublishOnly": "vessel prepublish",
    "login": "vessel login",
    "clean": "vessel clean",
    "build": "vessel build",
    "watch": "vessel build --watch",
    "check": "vessel check",
    "init": "lintest uninstall && lintest export",
    "lint": "lintest lint",
    "lint:fix": "lintest lint --fix",
    "test": "lintest test",
    "test:watch": "lintest test --watch",
    "test:coverage": "lintest test --coverage",
    "sonar": "sonar-scanner"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "files": [
    "dist",
    "!dist/test",
    "!dist/**/@setup.d.ts",
    "!dist/**/@setup.js",
    "!dist/**/*.test.d.ts",
    "!dist/**/*.test.js",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "devDependencies": {
    "@lintest/core": "^1.4.0",
    "@types/jest": "^28.1.3",
    "@types/node": "^17.0.45",
    "@types/webpack-env": "^1.17.0",
    "eslint": "^8.18.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "npm run check",
      "npm run lint"
    ]
  },
  "vessel": {
    "minVersion": "5.0.0"
  }
}
